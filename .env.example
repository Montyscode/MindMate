# Environment Variables for Deployment
# Copy this file to .env and fill in your values

# Database Configuration
DATABASE_URL=postgresql://user:password@host:port/database

# OpenAI Configuration (optional - app will run in demo mode without this)
OPENAI_API_KEY=your_openai_api_key_here

# Session Configuration (optional - will use default in development)
SESSION_SECRET=your_session_secret_here

# Deployment Configuration
NODE_ENV=production
PORT=5000

# Replit-specific variables (only needed when deploying to Replit)
# REPLIT_DOMAINS=your-repl-domain.replit.dev
# REPL_ID=your-repl-id
# ISSUER_URL=https://replit.com/oidc